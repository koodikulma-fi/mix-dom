{
    "name": "mix-dom",
    "version": "4.0.0",
    "description": "Powerful state based DOM rendering library (in JS/TS)",
    "keywords": [
        "MixDOM",
        "mix-dom",
        "mix",
        "mixing",
        "components",
        "dom",
        "render",
        "react",
        "user interface",
        "virtual dom",
        "front-end",
        "library",
        "framework"
    ],
    "homepage": "https://mixdomjs.org",
    "author": "Mikael Saur√©n",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mixdomjs/mixdom.git"
    },
    "module": "./dist/MixDOM.module.js",
    "main": "./dist/MixDOM.js",
    "types": "./dist/MixDOM.d.ts",
    "exports": {
        ".": {
            "import": "./dist/MixDOM.module.js",
            "require": "./dist/MixDOM.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "tsc": "tsc -p tsconfig.json",
        "build": "tsc && rollup -c rollup.config.js"
    },
    "dependencies": {
        "data-memo": "^1.0.0",
        "data-signals": "^1.0.1",
        "dom-types": "^1.0.0",
        "mixin-types": "^1.0.1"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.3.0",
        "rollup": "^2.77.2",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-terser": "^7.0.2",
        "typescript": "4.9"
    }
}
